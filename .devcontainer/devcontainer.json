{
  "name": "IRIS RealWorld example",
  "dockerComposeFile": [
    "../docker-compose.yml",
    "docker-compose.yml"
  ],
  "service": "dev",
  "shutdownAction": "none",
  "extensions": [
    "intersystems-community.vscode-objectscript"
  ],
  "runServices": [
    "web",
    "server"
  ],
  "forwardPorts": [
    52773,
    80
  ],
  "workspaceFolder": "/workspace",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/zsh",
    "objectscript.ignoreInstallServerManager": true
  }
}
